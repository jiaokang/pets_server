<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youlai.boot.pets.mapper.BusinessPetsMapper">

    <!-- 获取宠物信息分页列表 -->
    <select id="getBusinessPetsPage" resultType="com.youlai.boot.pets.model.vo.BusinessPetsVO">
        SELECT
                id,
                name,
                species,
                breed,
                birth_date,
                owner_id,
                profile_image_path,
                create_time,
                update_time
        FROM
            business_pets
        <where>
        </where>
    </select>

</mapper>
